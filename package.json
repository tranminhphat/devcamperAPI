{
  "name": "devcamperapi",
  "version": "1.0.0",
  "description": "Back-end API for Dev Camper project",
  "main": "index.js",
  "scripts": {
    "watch": "yarn copy&tsc -w",
    "copy": "copyfiles -u 1 src/public/**/* dist",
    "dev": "set NODE_ENV=development&&nodemon dist/index.js",
